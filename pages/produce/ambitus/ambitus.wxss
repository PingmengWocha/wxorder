page {
  width: 100%;
  height: 100%;
}

.content-item {
  width: 100%;
  padding-top: 10rpx;
  padding-bottom: 30rpx;
  background-color: #F2F4F5;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.item {
  width: 96%;
  display: flex;
  flex-direction: column;
  border-radius: 10rpx;
  background-color: white;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
}

.item-top {
  width: 100%;
  padding: 10rpx;
}

.img-out {
  width: 100%;
  padding-bottom: 10rpx;
}

.img {
  width: 100%;
}