page {
  width: 100%;
  height: 100%;
}

.root {
  width: 100%;
  height: 100%;
  background-color: #F2F4F5;
}

.top-search {
  height: 6%;
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  background-color: #F2F4F5;
}

.search-out {
  width: 90%;
  height: 100%;
  margin-top: 20rpx;
  border-radius: 180rpx;
  background-color: white;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.icon-search {
  width: 64rpx;
  height: 64rpx;
}



.top-content {
  height: 15%;
  width: 100%;
  display: flex;
  flex-direction: column;
  background-color: #F2F4F5;
}

.avatar-item {
  flex: 3;
  width: 100%;
  margin-top: 2%;
  overflow: hidden;
  white-space: nowrap;
  background-color: #F2F4F5;
}

.avatar-out {
  width: 30%;
  height: 100%;
  margin-right: 10rpx;
  margin-left: 10rpx;
  display: inline-block;
}

.fixscroll {
  display: flex;
  flex-wrap: nowrap;
  flex-direction: row;
  height: 100%;

}

.avatar {
  flex: 3;
  height: 100%;
  border-radius: 10rpx;
}

.text {
  flex: 2;
  font-size: 26rpx;
  margin-left: 5rpx;
}

.input-item {
  flex: 2;
  display: flex;
  flex-direction: row;
}

.input-out {
  margin-top: 2%;
  height: 100%;
  width: 50%;
  display: flex;
  flex-direction: row;
  background-color: white;
  /* border-top: 2rpx #c2bcbc solid;
  border-bottom: 2rpx #c2bcbc solid; */
}

.input-text {
  font-size: 32rpx;
  margin-left: 10rpx;
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.outinput {
  flex: 2;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.input {
  color: #c2bcbc;
}

.order-content {
  width: 100%;
  height: 83%;
  display: flex;
  flex-direction: column;
  padding-top: 20rpx;
}

.order-content-top {
  width: 100%;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  background-color: white;
  border-bottom: 2rpx #F2F4F5 solid;
}

.outtext {
  width: 25%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-content: center;
}

.tab-text {
  color: #1296db;
  font-size: 30rpx;
  text-align: center;
}

.order-item {
  width: 100%;
  height: 100%;
  overflow: hidden;
}

.sorder-item {
  width: 100%;
  height: 100%;
}

.item {
  height: 140rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
  background-color: white;
  margin-bottom: 10rpx;
}

.item.active {
  height: 140rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
  background-color: white;
  margin-bottom: 150rpx;
}

.left-img {
  flex: 1;
  height: 100%;
  border-top-left-radius: 10rpx;
  border-bottom-left-radius: 10rpx;
  margin-left: 5rpx;
}

.name-out{
  flex: 1;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.name {
  text-align: center;
}

.details {
  flex: 2;
  height: 100%;
  display: flex;
  flex-direction: row;
}

.price-out {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.price {
  text-align: center;
}

.details-other {
  flex: 1;
  display: flex;
  flex-direction: row;
  align-content: center;
  justify-content: space-around;
}

.plus-out {
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.plus {
  font-size: 48rpx;
}

.fouce-plus {
  background-color: #F2F4F5;
}

.other-out {
  display: flex;
  flex-direction: column;
  justify-content: center;
  
}

.other {
  border: 2rpx #c2bcbc solid;
  border-radius: 10rpx;
  padding: 10rpx;
  font-size: 24rpx;
}

.dialog-content-out {
  position: fixed;
  top: 0rpx;
  left: 0rpx;
  right: 0rpx;
  bottom: 0rpx;
  z-index: 10;
  background: rgba(0, 0, 0, 0);
  /* display: none; */
  width: 100%;
  height: 100%;

}

.dialog-content {
  position: fixed;
  width: 100%;
  background: #F2F4F5;
  top: 17%;
  height: 70%;
  border-bottom: 2rpx #A3A3A3 solid;
  box-sizing: border-box;
}

.top-like {
  width: 100%;
  height: 15%;
  border: 2rpx #A3A3A3 solid;
  background: white;
}

.stop-like {
  overflow: hidden;
  width: 100%;
  height: 100%;
}

.like-text {
  font-size: 28rpx;
}

.center-time {
  background: white;
  width: 100%;
  height: 10%;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.time-text {
  margin-left: 10rpx;
  margin-right: 20rpx;
  font-size: 34rpx;
}

.time {
  font-size: 32rpx;
  color: #c2bcbc;
}

.produce-list {
  width: 100%;
  height: 73%;
  border-top: 2rpx #A3A3A3 solid;
  background: #F2F4F5;
  overflow: hidden;
}

.sitem {
  width: 100%;
  height: 100%;
  overflow: hidden;
}

.like-item {
  width: 100%;
  height: 120rpx;
  display: flex;
  flex-direction: row;
  margin-bottom: 10rpx;
  background: white;
}

.produce-img-out{
  flex: 1;
  height: 100%;
}

.produce-img {
  height: 100%;
  width: 100%;
}

.produce-name-out {
  flex: 2;
  height: 100%;
  display: flex;
}

.produce-name {
  display: flex;
  flex-direction: column;
  justify-content: center;
  margin-left: 30rpx;
}

/* 菜品详情弹框 */
.food-dialog {
  position: fixed;
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background-color: rgba(0, 0, 0, 0);
  top: 0rpx;
  left: 0rpx;
  right: 0rpx;
  bottom: 0rpx;
  z-index: 100;
}

.food-dialog-content {
  width: 95%;
  height: 60%;
  background-color: white;
  display: flex;
  flex-direction: column;
  align-items: center;
  border-radius: 10rpx;
}

.top-title {
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  font-size: 36rpx;
  font-weight: 600;
  flex: 1;
}

.detail-content {
  flex: 6;
  width: 100%;
  display: flex;
  flex-direction: column;
}

.food-detail-item-out {
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  padding-top: 10rpx;
  padding-bottom: 10rpx;
}

.food-detail-title {
  flex: 1;
  margin-left: 20rpx;
}

.food-detail-content {
  flex: 4;
  margin-left: 20rpx;
}

.food-bottom-btn {
  flex: 2;
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.food-btn {
  color: #1AAD19;
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
}


.bus-bottom-content-out {
  position: fixed;
  bottom: 10rpx;
  width: 100%;
  height: 100rpx;
  background-color: rgba(0, 0, 0, 0);
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.bus-bottom-content {
  width: 80%;
  border-radius: 180rpx;
  display: flex;
  flex-direction: row;
  height: 100%;
}

.left-bus-content {
  flex: 3;
  background-color: white;
  border-top-left-radius: 180rpx;
  border-bottom-left-radius: 180rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.bus-left-image-out {
  flex: 1;
  height: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.bus-left-image {
  width: 50%;
  height: 50%;
}

.bus-left-item {
  flex: 4;
  height: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.bus-title {
  margin-left: 20rpx;
  margin-right: 20rpx;
}

.right-bus-content {
  flex: 1;
  background: linear-gradient(#B5CDFD,#6186E4);
  border-top-right-radius: 180rpx;
  border-bottom-right-radius: 180rpx;
}

.right-bus-text {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
}


